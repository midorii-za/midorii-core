web: target/universal/stage/bin/midorii-core -Dhttp.port=${PORT} -DapplyEvolutions.default=false -Ddb.default.driver=org.postgresql.Driver "-Ddb.default.url=${DATABASE_URL}?sslmode=require&ssl=true&sslfactory=org.postgresql.ssl.NonValidatingFactory" "-Ddb.readSlave.url=${READ_SLAVE_DATABASE_URL:-default DATABASE_URL}?sslmode=require&ssl=true&sslfactory=org.postgresql.ssl.NonValidatingFactory" -mem ${JAVA_MEMORY} -J-XX:+UseConcMarkSweepGC -J-XX:+CMSIncrementalMode -J-XX:+UseCompressedOops -J-XX:+UseCMSInitiatingOccupancyOnly -J-XX:CMSInitiatingOccupancyFraction=50